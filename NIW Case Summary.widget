{
  "version": "1.0",
  "name": "NIW Case Summary",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"children\":[{\"type\":\"Col\",\"gap\":2,\"children\":[{%- set _c -%},{\"type\":\"Row\",\"align\":\"center\",\"children\":[{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"sm\",\"children\":[]}]},{\"type\":\"Divider\",\"flush\":true,\"children\":[]}{%-for item in pathways -%},{\"type\":\"Row\",\"key\":{{ (item.id) | tojson }},\"gap\":3,\"align\":\"start\",\"children\":[{\"type\":\"Box\",\"background\":\"alpha-10\",\"radius\":\"sm\",\"padding\":2,\"children\":[{\"type\":\"Icon\",\"name\":{{ (item.icon) | tojson }},\"children\":[]}]},{\"type\":\"Col\",\"gap\":1,\"flex\":1,\"children\":[{\"type\":\"Row\",\"children\":[{\"type\":\"Text\",\"value\":{{ (item.label) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\",\"maxLines\":1,\"children\":[]},{\"type\":\"Spacer\",\"children\":[]},{\"type\":\"Badge\",\"label\":{{ (item.status) | tojson }},\"color\":{{ (item.badgeColor) | tojson }},\"children\":[]}]},{\"type\":\"Text\",\"value\":{{ (item.summary) | tojson }},\"size\":\"sm\",\"color\":\"secondary\",\"maxLines\":2,\"children\":[]}]},{\"type\":\"Button\",\"label\":\"Open\",\"variant\":\"outline\",\"size\":\"sm\",\"onClickAction\":{\"type\":\"niw.pathway.view\",\"payload\":{\"id\":{{ (item.id) | tojson }}}},\"children\":[]}]}{%-endfor-%},{\"type\":\"Divider\",\"children\":[]},{\"type\":\"Row\",\"children\":[{\"type\":\"Button\",\"label\":{{ ((\"Strengths (\" ~ strengthsCount ~ \")\")) | tojson }},\"iconStart\":\"star-filled\",\"variant\":\"soft\",\"color\":\"success\",\"onClickAction\":{\"type\":\"niw.strengths.view\"},\"children\":[]},{\"type\":\"Spacer\",\"children\":[]},{\"type\":\"Button\",\"label\":{{ ((\"Weaknesses (\" ~ weaknessesCount ~ \")\")) | tojson }},\"iconStart\":\"circle-question\",\"variant\":\"outline\",\"color\":\"warning\",\"onClickAction\":{\"type\":\"niw.weaknesses.view\"},\"children\":[]}]}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "widget",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "pathways": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "label": {
              "type": "string"
            },
            "icon": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "badgeColor": {
              "type": "string"
            },
            "summary": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "label",
            "icon",
            "status",
            "badgeColor",
            "summary"
          ],
          "additionalProperties": false
        },
        "minItems": 3
      },
      "strengthsCount": {
        "type": "number"
      },
      "weaknessesCount": {
        "type": "number"
      },
      "designSpec": {
        "type": "string"
      },
      "complexityBudget": {
        "type": "string"
      }
    },
    "required": [
      "title",
      "pathways",
      "strengthsCount",
      "weaknessesCount",
      "designSpec",
      "complexityBudget"
    ],
    "additionalProperties": false
  },
